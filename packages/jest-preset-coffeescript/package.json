{
	"name": "@heat/jest-preset-coffeescript",
	"version": "0.0.36",
	"license": "MIT",
	"repository": {
		"type": "git",
		"url": "https://github.com/heat-js/monorepo/tree/master/packages/jest-preset-coffeescript"
	},
	"scripts": {
		"build": "yarn run -T ncp-build",
		"patch": "npm version patch; npm publish --access=public",
		"minor": "npm version minor; npm publish --access=public",
		"major": "npm version major; npm publish --access=public",
		"prepublishOnly": "yarn run coffee src/build.coffee",
		"postpublish": "rm jest-preset.js; rm -rf dist"
	},
	"files": [
		"dist",
		"jest-preset.js"
	],
	"dependencies": {
		"@babel/core": "7.13.15",
		"babel-jest": "26.6.3",
		"coffeescript": "2.5.1"
	}
}
