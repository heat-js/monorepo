{
	"name": "@heat/code",
	"version": "0.0.32",
	"license": "MIT",
	"type": "module",
	"files": [
		"dist"
	],
	"bin": "./dist/bin.js",
	"main": "./dist/index.cjs",
	"module": "./dist/index.js",
	"types": "./dist/index.d.ts",
	"exports": {
		".": {
			"types": "./dist/index.d.ts",
			"require": "./dist/index.cjs",
			"import": "./dist/index.js"
		}
	},
	"repository": {
		"type": "git",
		"url": "https://github.com/heat-js/monorepo/tree/master/packages/code"
	},
	"scripts": {
		"test": "yarn node --experimental-vm-modules $(yarn bin jest)",
		"build": "yarn tsup src/index.ts src/bin.ts --format cjs,esm --dts --clean",
		"patch": "yarn build; npm version patch; npm publish --access=public",
		"minor": "yarn build; npm version minor; npm publish --access=public",
		"major": "yarn build; npm version major; npm publish --access=public"
	},
	"vitest": {
		"exclude": "./test/data/**"
	},
	"jest": {
		"transform": {
			"^.+\\.tsx?$": "ts-jest"
		},
		"testEnvironment": "node",
		"testMatch": [
			"**/test/**/*.[tj]s?(x)"
		],
		"testPathIgnorePatterns": [
			"/data/",
			"/dist/"
		]
	},
	"devDependencies": {
		"@heat/aws-lambda": "workspace:^",
		"jest": "^28.1.1",
		"ts-jest": "^29.0.3",
		"tsup": "^6.5.0",
		"uuid": "^9.0.0"
	},
	"dependencies": {
		"@babel/plugin-transform-runtime": "^7.19.6",
		"@babel/preset-env": "^7.20.2",
		"@babel/preset-react": "^7.18.6",
		"@rollup/plugin-babel": "^6.0.2",
		"@rollup/plugin-commonjs": "^23.0.2",
		"@rollup/plugin-json": "^5.0.1",
		"@rollup/plugin-node-resolve": "^15.0.1",
		"@rollup/plugin-typescript": "^9.0.2",
		"coffeescript": "^2.7.0",
		"commander": "^9.4.1",
		"object-assign": "^4.1.1",
		"rollup": "^3.2.5",
		"rollup-plugin-coffee-script": "^2.0.0",
		"rollup-plugin-uglify": "^6.0.4",
		"rollup-pluginutils": "^2.8.2",
		"tslib": "^2.4.1",
		"typescript": "^4.8.4",
		"vite": "^3.2.3",
		"vitest": "^0.25.1"
	}
}
