{
	"name": "@heat/aws-lambda",
	"main": "index",
	"version": "0.0.0",
	"license": "MIT",
	"type": "module",
	"repository": {
		"type": "git",
		"url": "https://github.com/heat-js/monorepo.git"
	},
	"bugs": {
		"url": "https://github.com/heat-js/monorepo/issues"
	},
	"exports": {
		".": {
			"import": "./src/index.js"
		},
		"./env": {
			"import": "./src/handlers/config/env.js"
		},
		"./validate": {
			"import": "./src/handlers/validate/types/index.js"
		}
	},
	"scripts": {
		"test": "yarn node --experimental-vm-modules $(yarn bin jest)",
		"test2": "yarn run -T jest --runInBand",
		"patch": "npm version patch; yarn run -T ncp",
		"minor": "npm version minor; yarn run -T ncp",
		"major": "npm version major; yarn run -T ncp"
	},
	"jest": {
		"preset": "ts-jest",
		"testEnvironment": "node",
		"testMatch": [
			"**/test/**/*.js"
		]
	},
	"dependencies": {
		"@aws-sdk/client-iot": "^3.204.0",
		"@aws-sdk/client-iot-data-plane": "^3.204.0",
		"@aws-sdk/client-lambda": "^3.202.0",
		"@aws-sdk/client-sns": "^3.204.0",
		"@aws-sdk/client-sqs": "^3.204.0",
		"@aws-sdk/client-ssm": "^3.204.0",
		"@aws-sdk/util-dynamodb": "^3.204.0",
		"@types/aws-lambda": "^8.10.108",
		"big.js": "^6.2.1",
		"chunk": "^0.0.3",
		"superstruct": "^0.16.7"
	},
	"devDependencies": {
		"@types/jest": "^29.2.2",
		"@types/node": "^18.11.9",
		"jest": "^29.2.2",
		"ts-jest": "^29.0.3",
		"typescript": "^4.8.4"
	}
}
