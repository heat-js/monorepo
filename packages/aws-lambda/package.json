{
	"name": "@heat/aws-lambda",
	"version": "0.0.45",
	"license": "MIT",
	"type": "module",
	"repository": {
		"type": "git",
		"url": "https://github.com/heat-js/monorepo.git"
	},
	"bugs": {
		"url": "https://github.com/heat-js/monorepo/issues"
	},
	"files": [
		"dist"
	],
	"main": "./dist/cjs/index.js",
	"module": "./dist/esm/index.js",
	"types": "./dist/esm/index.d.ts",
	"exports": {
		".": {
			"require": "./dist/cjs/index.js",
			"import": "./dist/esm/index.js",
			"types": "./dist/esm/index.d.ts"
		},
		"./environment": {
			"require": "./dist/cjs/services/environment/index.js",
			"import": "./dist/esm/services/environment/index.js",
			"types": "./dist/esm/services/environment/index.d.ts"
		},
		"./validate": {
			"require": "./dist/cjs/services/validate/index.js",
			"import": "./dist/esm/services/validate/index.js",
			"types": "./dist/esm/services/validate/index.d.ts"
		},
		"./dynamodb": {
			"require": "./dist/cjs/services/dynamodb/index.js",
			"import": "./dist/esm/services/dynamodb/index.js",
			"types": "./dist/esm/services/dynamodb/index.d.ts"
		},
		"./lambda": {
			"require": "./dist/cjs/services/lambda/index.js",
			"import": "./dist/esm/services/lambda/index.js",
			"types": "./dist/esm/services/lambda/index.d.ts"
		},
		"./sns": {
			"require": "./dist/cjs/services/sns/index.js",
			"import": "./dist/esm/services/sns/index.js",
			"types": "./dist/esm/services/sns/index.d.ts"
		},
		"./sqs": {
			"require": "./dist/cjs/services/sqs/index.js",
			"import": "./dist/esm/services/sqs/index.js",
			"types": "./dist/esm/services/sqs/index.d.ts"
		},
		"./iot": {
			"require": "./dist/cjs/services/iot/index.js",
			"import": "./dist/esm/services/iot/index.js",
			"types": "./dist/esm/services/iot/index.d.ts"
		},
		"./bigfloat": {
			"require": "./dist/cjs/services/bigfloat/index.js",
			"import": "./dist/esm/services/bigfloat/index.js",
			"types": "./dist/esm/services/bigfloat/index.d.ts"
		}
	},
	"scripts": {
		"test": "yarn run code test",
		"build-test": "yarn tsup src/index.ts src/services/environment/index.ts src/services/validate/index.ts src/services/dynamodb/index.ts src/services/lambda/index.ts src/services/sns/index.ts src/services/sqs/index.ts src/services/iot/index.ts src/services/bigfloat/index.ts --format cjs,esm --dts --clean",
		"build-lol": "yarn tsc --outDir ./dist/esm; yarn tsc --module commonjs --outDir ./dist/cjs",
		"build": "yarn tsc --build --clean",
		"publish": "yarn build; yarn npm publish --access=public"
	},
	"vitest": {
		"exclude": [
			"test/aws/**/*",
			"test/bundle/**/*"
		]
	},
	"dependencies": {
		"@aws-sdk/client-dynamodb": "^3.226.0",
		"@aws-sdk/client-iot": "^3.204.0",
		"@aws-sdk/client-iot-data-plane": "^3.204.0",
		"@aws-sdk/client-lambda": "^3.202.0",
		"@aws-sdk/client-sns": "^3.204.0",
		"@aws-sdk/client-sqs": "^3.204.0",
		"@aws-sdk/client-ssm": "^3.204.0",
		"@aws-sdk/lib-dynamodb": "^3.226.0",
		"@aws-sdk/util-dynamodb": "^3.226.0",
		"@aws-sdk/util-utf8-node": "^3.208.0",
		"@types/aws-lambda": "^8.10.108",
		"aws-sdk": "2.1083.0",
		"big.js": "^6.2.1",
		"bigfloat-esnext": "^3.0.1",
		"chunk": "^0.0.3",
		"superstruct": "^1.0.3"
	},
	"devDependencies": {
		"@heat/aws-test": "^0.0.8",
		"@heat/code": "^0.0.27",
		"@types/chunk": "^0.0.0",
		"@types/node": "^18.11.9",
		"tsup": "^6.5.0",
		"typescript": "^4.9.4"
	}
}
